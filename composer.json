{
    "name": "fm-labs/cakephp-sugar",
    "description": "Sugar plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "php": ">=8.2",
        "cakephp/cakephp": "^5"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "Cake\\": "./vendor/cakephp/cakephp",
            "Sugar\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sugar\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
